                            <script language="4">
                                <content>
                                    <![CDATA[from naoqi import ALProxy

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.tts = ALProxy('ALTextToSpeech')
        self.memProxy = ALProxy('ALMemory')
        self.people = ALProxy('ALPeoplePerception')
        self.people.setTimeBeforeVisiblePersonDisappears(1)
        self.facechar = ALProxy('ALFaceCharacteristics')

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #self.onStopped() #activate the output of the box
        #self.memProxy.unsubscribeToEvent('FaceDetected', 'ALFaceDetection')
        #self.tts.say('Trying')
        ppl = self.memProxy.getData('PeoplePerception/VisiblePeopleList')
        if len(ppl) < 1:
            self.tts.say('No face detected')
            self.onStopped()
        else:
            id = ppl[-1]
            b = self.facechar.analyzeFaceCharacteristics(id)
            self.log(b)
            if b:
                #self.tts.say('Done')
                self.log(ppl)
                #id = self.memProxy.getData('FaceDetected')[1][0][1][0]
                #self.log(id)
                #info = self.memProxy.getData('PeoplePerception/Person')
                #self.log(info)
                #threshold = self.memProxy.getData('PeoplePerception/Person/' + str(id) + '/SmileProperties') #self.facechar.getSmilingThreshold()
                #self.tts.say('Threshold ' + str(round(threshold[0], 2)))
                emotions = self.memProxy.getData('PeoplePerception/Person/' + str(id) + '/ExpressionProperties')
                self.log(emotions)
                # 0 - neutral
                # 1 - happy
                # 2 - surprise
                # 3 - angry
                # 4 - sad
                emotion_index = emotions.index(max(emotions))
                map = {0: 'neutral', 1: 'happy', 2: 'surprised', 3: 'angry', 4: 'sad'}
                self.tts.say('You are feeling ' + map[emotion_index])
                reward = emotions[0] * 5 + emotions[1] * 10 + emotions[2] * 5 - emotions[3] * 10 - emotions[4] * 5
                self.memProxy.insertData('reward', round(reward, 2))
                self.tts.say('My reward is ' + str(round(self.memProxy.getData('reward'), 2)))
            self.onStopped()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
</content>
