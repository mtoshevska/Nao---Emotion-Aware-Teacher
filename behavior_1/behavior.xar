                            <script language="4">
                                <content>
                                    <![CDATA[import random

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.memProxy = ALProxy('ALMemory')

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        #self.onStopped() #activate the output of the box
        if self.memProxy.getData('init') != 0:
            r = round(self.memProxy.getData('reward'), 2)
            current_state = self.memProxy.getData('currentState')
            gamma_param = 0.9
            # learning rate
            alpha_param = 0.5
            # probability of taking a random action
            epsilon_param = 0.1
            number_of_actions = 3
            q = self.memProxy.getData('q')
            q = np.array(q)
            next_action = self.choose_action(q, current_state, epsilon_param, number_of_actions)
            self.log('New state: ' + str(next_action))
            next_state = next_action
            # update Q
            q = self.update_q(current_state, next_state, next_action, gamma_param, q, r, alpha_param)
            q = q.tolist()
            self.memProxy.insertData('q', q)
            self.log(q)
            # change state
            current_state = next_action
            #current_state = 1
            self.log(current_state)
            self.memProxy.insertData('currentState', current_state)
            if current_state == 0:
                self.teach()
            elif current_state == 1:
                self.solve()
            else:
                self.play()
        else:
            self.log('x')
            self.memProxy.insertData('init', 1)
            self.teach()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def update_q(self, state, next_s, action, gamma, q_matrix, reward, alpha):
        best_action = np.argmax(q_matrix[next_s])
        target = reward + gamma * q_matrix[next_s, best_action]
        delta = target - q_matrix[state, action]
        q_matrix[state, action] += alpha * delta
        self.log('max_value: ' + str(target))
        return q_matrix

    def choose_action(self, q_matrix, state, epsilon, num_actions):
        # choose next action using epsilon greedy policy
        a = np.ones(num_actions, dtype=float) * epsilon / num_actions
        best_action = np.argmax(q_matrix[state])
        a[best_action] += (1.0 - epsilon)
        action = random.choice(np.arange(len(a)).tolist())
        return action]]>
